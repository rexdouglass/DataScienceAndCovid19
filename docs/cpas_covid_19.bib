  file = {/home/skynet2/snap/zotero-snap/common/Zotero/storage/JXIVVS5T/Smits and Permanyer - 2019 - The Subnational Human Development Database.pdf;/home/skynet2/snap/zotero-snap/common/Zotero/storage/SQHASX5B/sdata201938.html},
