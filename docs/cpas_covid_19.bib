@techreport{gandalCOVID19DeathsWeekendEffect2020,
  title = {U.{{S}}. {{COVID}}-19 {{Deaths}}: {{The Weekend}}-{{Effect}} '{{Mystery}}'},
  shorttitle = {U.{{S}}. {{COVID}}-19 {{Deaths}}},
  author = {Gandal, Keith and Gandal, Neil},
  year = {2020},
  month = jul,
  address = {{Rochester, NY}},
  institution = {{Social Science Research Network}},
  doi = {10.2139/ssrn.3654952},
  abstract = {In this paper, we examine U.S. COVID-19 deaths by day of the week. Mysteriously, the same pattern has repeated every single week of the pandemic: deaths rise from Tuesday-Friday and come down Saturday-Monday, hitting a nadir on Sunday or Monday. We show that such deaths are substantially higher during the Tuesday to Friday period than they are during the Saturday to Monday period. Controlling for time trends, deaths during the Tuesday to Friday period are at least 40 percent higher than the Saturday to Monday period.   The weekend effect does not obtain in New York City. Without New York City, controlling for time trends, deaths during the Tuesday to Friday period in the U.S. are 50 percent higher than the Saturday to Monday period. This is much larger than the ``weekend'' effect for the rest of the world (approximately 20 percent.) We provide some preliminary thoughts as to why this obtains in the case of COVID-19 deaths in the U.S., but not those in New York City. Further research is encouraged.},
  keywords = {coronavirus,COVID-19,COVID-19 Deaths,New York Deaths,U.S. Deaths,Worldwide Deaths},
  language = {en},
  number = {ID 3654952},
  type = {{{SSRN Scholarly Paper}}}
}

