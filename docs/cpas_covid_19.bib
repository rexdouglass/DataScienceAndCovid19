@article{leungRespiratoryVirusShedding2020,
  title = {Respiratory Virus Shedding in Exhaled Breath and Efficacy of Face Masks},
  author = {Leung, Nancy H. L. and Chu, Daniel K. W. and Shiu, Eunice Y. C. and Chan, Kwok-Hung and McDevitt, James J. and Hau, Benien J. P. and Yen, Hui-Ling and Li, Yuguo and Ip, Dennis K. M. and Peiris, J. S. Malik and Seto, Wing-Hong and Leung, Gabriel M. and Milton, Donald K. and Cowling, Benjamin J.},
  year = {2020},
  month = may,
  volume = {26},
  pages = {676--680},
  publisher = {{Nature Publishing Group}},
  issn = {1546-170X},
  doi = {10.1038/s41591-020-0843-2},
